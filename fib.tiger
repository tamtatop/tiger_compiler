/* Test: user function call with 8 arguments */

program function_call_8_arg
let
begin

    function sum (a : int, b : int, c : int, d : int, e : int, f : int, g : int, h : int) : int
    begin
        return a + b + c + d + e + f + g + h;
    end

    function main() : int
    begin
        let
            var result : int;
        begin
            result := sum(1,2,3,4,5,6,7,8);
            printi(result);
        end
        return 0;
    end

end
