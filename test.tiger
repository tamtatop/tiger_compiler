/* selection sort */
program sort
let
    type catList = array[8] of int;
    static arr : catList;
    static x : int ;
    static t : int ;
begin
    function main (x: int) : int
    begin
        let
            var x: int;
        begin
            t:=x;
        end
        let
            var x: int;
        begin
            t:=x;
            x := 5;
            while (x > 0) do
                printi(x);
                x := x-1;
            enddo;
        end
        arr[x] := t;
    end
end
