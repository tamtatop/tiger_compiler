program basic_loop_forloop
let
begin

    function main() : int
    begin
        let
            var a : int;
            var b : int;
            var c : int;
        begin
            c := 1;
            a := 3;
            b := 4;
            for a := 1 to b do
                c := a * a;
            enddo;
            return c;
        end
        return 0;
    end

end

