/* selection sort */
program sort
let
    type catList = array[8] of int;
    static arr : catList;
    static x : int ;
    static t : int ;
begin
    function main (x: int) : int
    begin
        let
            var x: int;
        begin
            t:=x;
        end
        let
            var x: int;
        begin
            t:=x;
        end
        arr[x] := t;
    end
end
